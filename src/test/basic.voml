
@import json "a" as a

@import json "a" {

}

key = "value"
maybe = Some(1)



[a]
	[>b1]
	key = "a.b1.key"
	[^b2]  # {^.b2}
	key = "a.b2.key"
[<b1]  // same as [<][>b1]
key = "a.b1.key"
	[>c1]
	key = "a.b1.c1.key"

--- # 返回顶级

connection_max.a = 5cm
v = [
	@merge(override)
	@merge_as_source(unset)
	@merge_as_target(ignore)
	a = Some(1)
    b = None()
]

[name]
  . a = 2
  * a
  * b


